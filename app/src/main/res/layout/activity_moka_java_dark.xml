<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MokaJavaDarkActivity">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="421dp"
        android:layout_height="111dp"
        android:text="Moka Java Dark"
        android:textAlignment="center"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="542dp"
        android:layout_height="489dp"
        android:text="Description:\n\nMedium-dark roasted blend of unique Arabica coffees combining Ethiopians with intense floral bouquet together with more balanced Java beans.\n\n\nTasting Notes:\n\nCocoa, Floral Notes, Syrupy Body"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button1"
        android:layout_width="200dp"
        android:layout_height="135dp"
        android:backgroundTint="@android:color/secondary_text_dark"
        android:onClick="buttonPressMokaJavaEthiopian"
        android:padding="20dip"
        android:text="Moka Java Ethiopian\n\n95% Simliarity"
        android:textColor="@android:color/tertiary_text_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="200dp"
        android:layout_height="140dp"
        android:backgroundTint="@android:color/secondary_text_dark"
        android:onClick="buttonPressOrganicFairTradeMillenniumJoe"
        android:padding="20dip"
        android:text="Organic Fair Trade Millennium Joe\n\n25% Simliarity"
        android:textColor="@android:color/tertiary_text_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <Button
        android:id="@+id/button3"
        android:layout_width="200dp"
        android:layout_height="135dp"
        android:backgroundTint="@android:color/secondary_text_dark"
        android:onClick="buttonPressOrganicHawaiian"
        android:padding="20dip"
        android:text="Organic Hawaiian\n\n25% Simliarity"
        android:textColor="@android:color/tertiary_text_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="200dp"
        android:layout_height="135dp"
        android:backgroundTint="@android:color/secondary_text_dark"
        android:onClick="buttonPressOrganicItalianRoast"
        android:padding="20dip"
        android:text="Organic Italian Roast\n\n21% Simliarity"
        android:textColor="@android:color/tertiary_text_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button5"
        android:layout_width="200dp"
        android:layout_height="135dp"
        android:backgroundTint="@android:color/secondary_text_dark"
        android:onClick="buttonPressOrganicRoadWarriorBlend"
        android:padding="10dip"
        android:text="Organic Road Warrior Blend\n\n19% Simliarity"
        android:textColor="@android:color/tertiary_text_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

</androidx.constraintlayout.widget.ConstraintLayout>